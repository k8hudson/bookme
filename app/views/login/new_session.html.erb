<h1>Log in to BookMe</h1>

<%= link_to "Sign in with Facebook", "/auth/facebook" %>


<h2>User</h2>
<%= form_for @user, url: login_path do |f| %>
<%= f.error_messages %>

<div>
  Email: <%= f.text_field :email %>
</div>

<div>
  Password: <%= f.password_field :password %>
</div>

<%= f.submit "Log in" %>

<% end %>
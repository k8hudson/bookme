<h1>Create a User Account</h1>


<%= form_for @user do |f| %>
<%= f.error_messages %>

<div>
<%= f.label :username, "Name" %>
<%= f.text_field :username %>
</div>

<div>
<%= f.label :email %>
<%= f.text_field :email %>
</div>

<div>
<%= f.label :password %>
<%= f.password_field :password %>
</div>

<div>
<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %>
</div>

<%= f.submit %>
<% end %>